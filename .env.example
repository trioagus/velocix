APP_NAME=Velocix
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost:8000
APP_KEY=

# Database Configuration
# Options: mysql, pgsql, sqlite
DB_CONNECTION=sqlite
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=database/database.sqlite
DB_USERNAME=root
DB_PASSWORD=

# MySQL Example:
# DB_CONNECTION=mysql
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=velocix
# DB_USERNAME=root
# DB_PASSWORD=secret

# PostgreSQL Example:
# DB_CONNECTION=pgsql
# DB_HOST=127.0.0.1
# DB_PORT=5432
# DB_DATABASE=velocix
# DB_USERNAME=postgres
# DB_PASSWORD=secret

# Cache
CACHE_DRIVER=file

# Session
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Vite
VITE_APP_NAME="${APP_NAME}"
VITE_APP_URL="${APP_URL}"